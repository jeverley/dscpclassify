        ip6 dscp set af42
    }

    chain dscp_set_af43 {
        ip dscp set af43
        ip6 dscp set af43
    }

    chain dscp_set_cs5 {
        ip dscp set cs5
        ip6 dscp set cs5
    }

    chain dscp_set_va {
        ip dscp set $va
