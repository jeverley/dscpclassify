table inet dscpclassify {
    ## IP version agnostic DSCP set chains
    chain dscp_set_cs0 {
        ip dscp set cs0
        ip6 dscp set cs0
    }

    chain dscp_set_le {
        ip dscp set $lephb
        ip6 dscp set $lephb
    }

    chain dscp_set_cs1 {
        ip dscp set cs1
        ip6 dscp set cs1
    }

    chain dscp_set_af11 {
